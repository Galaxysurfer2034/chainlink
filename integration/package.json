    "cross-env": "^6.0.3",
    "cypress": "^3.4.1",
    "depcheck": "^0.8.3",
    "eslint": "^6.7.2",
    "prettier": "^1.18.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "webpack": "^4.41.2"
  },
  "prettier": "@chainlink/prettier-config"
