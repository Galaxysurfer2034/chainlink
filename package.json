{
  "name": "linkstats",
  "private": true,
  "version": "0.0.0",
  "description": "LINK Stats",
  "author": "Chainlink Dev Team",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "dev": "concurrently \"cd client && yarn start\" \"ts-node-dev --respawn --transpileOnly ./src/index.ts\"",
    "build": "cd client && yarn build",
    "prod": "yarn migration:run && ts-node ./src/index.ts",
    "test": "yarn jest --detectOpenHandles",
    "lint": "eslint --color --ext .js --ext .jsx --ext .ts --ext .tsx .",
    "lint:fix": "eslint --fix --color --ext .js --ext .jsx --ext .ts --ext .tsx .",
    "schema:sync": "ts-node ./node_modules/.bin/typeorm schema:sync",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "test:db:prepare": "yarn schema:sync -c test"
  },
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/smartcontractkit/linkstats"
  },
  "engines": {
    "node": "^11.12"
  },
  "dependencies": {
    "express": "^4.16.4",
    "pg": "^7.8.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.2.0",
    "ts-node": "^8.0.3",
    "typeorm": "^0.2.15",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^1.5.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/material-ui": "^0.21.6",
    "@types/node": "^11.11.3",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "^2.0.7",
    "@types/whatwg-fetch": "^0.0.33",
    "babel-jest": "23.6.0",
    "concurrently": "^4.1.0",
    "eslint": "5.12.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-prettier-standard": "^2.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.16.4",
    "supertest": "^4.0.2",
    "ts-jest": "^23.0.0",
    "ts-node-dev": "^1.0.0-pre.32"
  }
}
