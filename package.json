{
  "private": true,
  "workspaces": [
    "chainlink",
    "operator-gui",
    "example-echo-server",
    "examples-twilio-sms"
  ],
  "scripts": {
    "start": "cd gui && react-static start",
    "build": "cd gui && cross-env NODE_OPTIONS=--max_old_space_size=2048 react-static build",
    "serve": "cd gui && serve dist -p 3000",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "eslint --fix '**/*.js'",
    "lint-ci": "eslint '**/*.js'",
    "tslint": "tslint -c tslint.json '**/*.ts' '**/*.tsx'",
    "test": "cross-env NODE_ENV=test jest",
    "test-e2e": "cross-env NODE_ENV=test yarn jest e2e/ --runInBand",
    "test-sol": "./internal/ci/truffle_test",
    "watch": "cross-env NODE_ENV=test jest --watchAll --notify",
    "flatten": "./internal/bin/flatten-ropsten"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-react-tools": "1.x.x",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.3.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-react-tools": "1.x.x",
    "eslint-config-standard": "^12.0.0",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-wallet": "^0.6.2",
    "ethjs": "^0.4.0",
    "ethlint": "^1.2.3",
    "expect-puppeteer": "^4.1.0",
    "express": "^4.16.4",
    "jest": "^24.1.0",
    "jest-silent-reporter": "^0.1.0",
    "link_token": "^1.0.3",
    "openzeppelin-solidity": "^1.12.0",
